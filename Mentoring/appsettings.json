{
    "Logging": {
        "LogLevel": {
            "Default": "Information",
            "Microsoft.AspNetCore": "Information"
        }
    },
    "Log4NetCore": {
        "PropertyOverrides": [
            {
                "XPath": "/log4net/root/level",
                "Attributes": {
                    "value": "WARN"
                }
            }
        ]
    },
    "AllowedHosts": "*",
    "MaxProductsToShow": "10",
    "CacheExpiration": "5",
    "MaxImagesOnCache" : "5"
}
